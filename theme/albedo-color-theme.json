{
    "$schema": "vscode://schemas/color-theme",
    "name": "Albedo",
    "tokenColors":[
        {
            "name": "Comments",
            "scope": [
                "comment",
                "punctuation.definition.comment"
            ],
            "settings": {
                "foreground": "#888"
            }
        },
        {
            "name": "Comments: Documentation",
            "scope": [
                "comment.documentation",
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#888"
            }
        },
        {
            "name": "Comments: Preprocessor",
            "scope": "comment.block.preprocessor",
            "settings": {
                "foreground": "#888"
            }
        },
        {
            "name": "Invalid - Illegal",
            "scope": "invalid.illegal",
            "settings": {
                "foreground": "#f31313"
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword",
                "storage"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#000"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language",
                "support.constant",
                "variable.language"
            ],
            "settings": {}
        },
        {
            "name": "Operators",
            "scope": "keyword.operator",
            "settings": {}
        },
        {
            "name": "Types",
            "scope": [
                "storage.type",
                "support.type"
            ],
            "settings": {}
        },
        {
            "name": "Variables",
            "scope": [
                "variable",
            ],
            "settings": {}
        },
        {
            "name": "Functions",
            "scope": [
                "entity.name.function",
                "support.function"
            ],
            "settings": {
                "fontStyle": "bold",
                "foreground": "#25437afa"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type",
                "support.class"
            ],
            "settings": {
                "fontStyle": "underline",
            }
        },
        {
            "name": "Exceptions",
            "scope": "entity.name.exception",
            "settings": {}
        },
        {
            "name": "Sections",
            "scope": "entity.name.section",
            "settings": {
                "fontStyle": "underline"
            }
        },
        {
            "name": "Numbers, Characters",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant"
            ],
            "settings": {}
        },
        {
            "name": "Strings",
            "scope": "string",
            "settings": {
                "foreground": "#3a3a3a"
            }
        },
        {
            "name": "Strings: Escape Sequences",
            "scope": "constant.character.escape",
            "settings": {}
        },
        {
            "name": "Strings: Regular Expressions",
            "scope": "string.regexp",
            "settings": {}
        },
        {
            "name": "Strings: Symbols",
            "scope": "constant.other.symbol",
            "settings": {}
        },
        {
            "name": "Punctuation",
            "scope": "punctuation",
            "settings": {}
        }
    ],
    "colors": {
        "activityBar.background": "#EEE",
        "activityBar.foreground": "#007ADD",
        "editor.background": "#FFF",
        "editor.foreground": "#000",
        "editor.findMatchBackground": "#FFCC6D",
        "editor.lineHighlightBackground": "#00000004",
        "editor.selectionBackground": "#B3D7FF",
        "editor.selectionHighlightBackground": "#E1E1E1",
        "editor.findMatchHighlightBackground": "#FEE8A6",
        "editorCursor.foreground": "#007ADD",
        "editorGroupHeader.tabsBackground": "#F0F0F0",
        "editorLineNumber.foreground": "#9DA28A",
        "panel.background": "#EEE",
        "sideBar.background": "#EEE",
        "statusBar.background": "#DDDDDD",
        "statusBar.foreground": "#424242",
        "statusBar.debuggingBackground": "#FFCC6D",
        "statusBar.debuggingForeground": "#000",
        "statusBar.noFolderBackground": "#000",
        "statusBar.noFolderForeground": "#ececec",
        "terminal.ansiWhite": "#FFF",
        "terminal.ansiBlack": "#000000",
        "terminal.ansiBlue": "#325CC0",
        "terminal.ansiCyan": "#0083B2",
        "terminal.ansiGreen": "#448C27",
        "terminal.ansiMagenta": "#7A3E9D",
        "terminal.ansiRed": "#AA3731",
        "terminal.ansiYellow": "#CB9000",
        "terminal.ansiBrightWhite": "#FFFFFF",
        "terminal.ansiBrightBlack": "#777777",
        "terminal.ansiBrightBlue": "#007ACC",
        "terminal.ansiBrightCyan": "#00AACB",
        "terminal.ansiBrightGreen": "#60CB00",
        "terminal.ansiBrightMagenta": "#E64CE6",
        "terminal.ansiBrightRed": "#F05050",
        "terminal.ansiBrightYellow": "#FFBC5D"
    }
}
